		"sass": "^1.92.1",
		"shiki": "^3.12.2",
		"vite": "^6.3.5",
		"vite-plugin-dts": "^4.5.4",
		"vue": "^3.5.21"
	},
	"peerDependencies": {
		"shiki": "*",
		"vitepress": "*",
		"vue": "^3.2.0"
