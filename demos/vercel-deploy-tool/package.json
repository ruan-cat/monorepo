		"shx": "^0.3.4",
		"vercel": "^34.2.7"
	},
	"devDependencies": {
		"@types/cpx": "^1.5.5",
		"@types/gulp": "^4.0.17",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^22.5.1",
		"@vitest/ui": "^2.0.5",
		"vitest": "^2.0.5"
	}
