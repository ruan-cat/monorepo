{
	"name": "@ruan-cat/vite-plugin-ts-alias",
	"version": "0.1.0",
	"description": "将 tsconfig.paths 配置转换成 vite 的 alias 路径别名。",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"homepage": "https://github.com/ruan-cat/monorepo/tree/dev/vite-plugins/vite-plugin-ts-alias",
	"bugs": {
		"url": "https://github.com/ruan-cat/monorepo/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ruan-cat/monorepo.git",
		"directory": "vite-plugins/vite-plugin-ts-alias"
	},
	"scripts": {
		"build": "tsup",
		"prebuild": "automd"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"keywords": [
		"vite",
		"vite plugin",
		"tsconfig paths",
		"alias"
	],
	"author": {
		"name": "ruan-cat",
		"email": "1219043956@qq.com",
		"url": "https://github.com/ruan-cat"
	},
	"license": "MIT",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/",
		"tag": "beta"
	},
	"files": [
		"!**/.vercel/**",
		"!**/.vitepress/**",
		"dist/**",
		"tsconfig.json",
		"README.md",
		"!src/**/docs/**",
		"!src/**/tests/**",
		"!*.test.*",
		"!src/**/*.md"
	],
	"peerDependencies": {
		"vite": ">=6"
	},
	"dependencies": {
		"consola": "^3.4.2",
		"load-tsconfig": "^0.2.5",
		"upath": "^2.0.1"
	},
	"devDependencies": {
		"automd": "^0.4.2",
		"tsup": "^8.5.0",
		"typescript": "^4.9.5",
		"vite": "^6.3.5"
	}
}
