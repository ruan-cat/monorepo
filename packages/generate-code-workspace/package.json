{
	"name": "@ruan-cat/generate-code-workspace",
	"version": "0.2.1",
	"description": "根据 pnpm-workspace.yaml 配置，生成vscode的code-workspace工作区文件。",
	"type": "module",
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"homepage": "https://github.com/ruan-cat/vercel-monorepo-test/tree/main/packages/generate-code-workspace/readme.md",
	"bugs": {
		"url": "https://github.com/ruan-cat/vercel-monorepo-test/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ruan-cat/vercel-monorepo-test.git",
		"directory": "packages/generate-code-workspace"
	},
	"scripts": {
		"clear": "rimraf dist .turbo .vercel .cache .temp",
		"rm:node_modules": "rimraf node_modules"
	},
	"exports": {
		".": {
			"import": "./src/index.ts",
			"types": "./src/index.ts"
		},
		"./src/*": "./src/*"
	},
	"keywords": [
		"code-workspace",
		"vscode"
	],
	"author": {
		"name": "ruan-cat",
		"email": "1219043956@qq.com",
		"url": "https://github.com/ruan-cat"
	},
	"license": "MIT",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/",
		"tag": "beta"
	},
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./*"
			]
		}
	},
	"files": [
		"dist",
		"src",
		"CHANGELOG.md",
		"readme.md"
	],
	"dependencies": {
		"commander": "^12.1.0",
		"consola": "^3.2.3",
		"glob": "^11.0.0",
		"jsonc-parser": "^3.3.1",
		"lodash-es": "^4.17.21",
		"yaml": "^2.6.0"
	},
	"devDependencies": {
		"pkg-types": "^1.2.1"
	}
}