{
	"name": "@ruan-cat/generate-code-workspace",
	"version": "0.4.13",
	"description": "根据 pnpm-workspace.yaml 配置，生成vscode的code-workspace工作区文件。",
	"type": "module",
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"homepage": "https://github.com/ruan-cat/monorepo/tree/main/packages/generate-code-workspace/README.md",
	"bugs": {
		"url": "https://github.com/ruan-cat/monorepo/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ruan-cat/monorepo.git",
		"directory": "packages/generate-code-workspace"
	},
	"scripts": {},
	"exports": {
		".": {
			"import": "./src/index.ts",
			"types": "./src/index.ts"
		},
		"./src/*": "./src/*"
	},
	"keywords": [
		"code-workspace",
		"vscode"
	],
	"author": {
		"name": "ruan-cat",
		"email": "1219043956@qq.com",
		"url": "https://github.com/ruan-cat"
	},
	"license": "MIT",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/",
		"tag": "beta"
	},
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./*"
			]
		}
	},
	"files": [
		"dist",
		"src"
	],
	"dependencies": {
		"@ruan-cat/utils": "workspace:^",
		"commander": "^13.1.0",
		"consola": "^3.4.2",
		"glob": "^11.0.3",
		"js-yaml": "^4.1.0",
		"jsonc-parser": "^3.3.1",
		"lodash-es": "^4.17.21"
	},
	"devDependencies": {
		"@types/js-yaml": "^4.0.9",
		"automd": "^0.4.0",
		"pkg-types": "^2.3.0"
	},
	"peerDependencies": {
		"tsx": "4.19.3"
	},
	"peerDependenciesMeta": {
		"tsx": {
			"optional": true
