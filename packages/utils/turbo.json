{
	"extends": [
		"//"
	],
	"tasks": {
		"clean-docs": {
			"cache": false
		},
		"copy-src": {
			"cache": false,
			"dependsOn": [
				"clean-docs"
			]
		},
		"copy-readme": {
			"cache": false,
			"dependsOn": [
				"clean-docs"
			]
		},
		"do-copy": {
			"dependsOn": [
				"copy-src",
				"copy-readme"
			]
		},
		"typedoc": {
			"cache": false
		},
		"generate-md": {
			"dependsOn": [
				"do-copy",
				"typedoc"
			]
		},
		"std-prepare": {
			"dependsOn": [
				"build",
				"generate-md"
			]
		},
		"docs:dev-main": {
			"dependsOn": [
				"std-prepare"
			]
		},
		"docs:dev": {
			"dependsOn": [
				"docs:dev-main"
			]
		},
		"build:docs-main": {
			"dependsOn": [
				"std-prepare"
			]
		},
		"do-build-docs": {
			"dependsOn": [
				"build:docs-main"
			]
		}
	}
}