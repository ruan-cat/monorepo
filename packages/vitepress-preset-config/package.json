		"@nolebase/vitepress-plugin-highlight-targeted-heading": "^2.15.1",
		"lodash-es": "^4.17.21",
		"vitepress": "^1.6.3",
		"vitepress-demo-plugin": "^1.3.1",
		"vitepress-sidebar": "^1.31.1",
		"vue": "^3.5.13"
	},
	"devDependencies": {
		"@types/lodash-es": "^4.17.12",
		"tsup": "^8.4.0"
	},
	"bugs": {
		"url": "https://github.com/ruan-cat/vercel-monorepo-test/issues"
	},
	"repository": {
