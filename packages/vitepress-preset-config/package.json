{
	"name": "@ruan-cat/vitepress-preset-config",
	"version": "2.1.0",
	"description": "用于给大多数的vitepress项目提供一个预设的配置文件。",
	"homepage": "https://vitepress-preset.ruancat6312.top",
	"types": "./src/config.mts",
	"main": "./src/config.mts",
	"scripts": {
		"docs:dev": "vitepress dev src/docs --port 8080",
		"build:docs": "vitepress build src/docs",
		"build": "tsup",
		"prebuild": "automd"
	},
	"dependencies": {
		"@leelaa/vitepress-plugin-extended": "^1.0.7",
		"@nolebase/vitepress-plugin-git-changelog": "^2.18.2",
		"@ruan-cat/utils": "workspace:^",
		"@shikijs/vitepress-twoslash": "^3.12.0",
		"consola": "^3.4.2",
		"js-yaml": "^4.1.0",
		"lodash-es": "^4.17.21",
		"markdown-it-async": "^2.2.0",
		"markdown-it-mathjax3": "^4.3.2",
		"vitepress-demo-plugin": "^1.4.7",
		"vitepress-plugin-llms": "^1.7.3",
		"vitepress-sidebar": "^1.33.0",
		"vitepress-theme-teek": "^1.4.6",
		"vue": "^3.5.20"
	},
	"devDependencies": {
		"@types/lodash-es": "^4.17.12",
		"automd": "^0.4.0",
		"element-plus": "^2.11.1",
		"tsup": "^8.5.0",
		"vitepress": "^1.6.4"
	},
	"peerDependencies": {
		"vitepress": "^1.6.x",
		"vitepress-demo-plugin": "^1",
		"vue": "^3.5.x"
	},
	"peerDependenciesMeta": {
		"vitepress": {
			"optional": true
		},
		"vue": {
			"optional": true
		},
		"vitepress-demo-plugin": {
			"optional": false
		}
	},
	"bugs": {
		"url": "https://github.com/ruan-cat/monorepo/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ruan-cat/monorepo.git",
		"directory": "packages/vitepress-preset-config"
	},
	"exports": {
		"./config": {
			"import": "./dist/config.mjs",
			"types": "./src/config.mts"
		},
		"./theme": {
			"import": "./src/theme.ts",
			"types": "./src/theme.ts"
		},
		"./src/*": "./src/*"
	},
	"keywords": [
		"vitepress"
	],
	"author": {
		"name": "ruan-cat",
		"email": "1219043956@qq.com",
		"url": "https://github.com/ruan-cat"
	},
	"license": "MIT",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/",
		"tag": "beta"
	},
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./*"
			]
		}
	},
	"files": [
		"!**/.vercel/**",
		"dist",
		"src",
		"!src/docs/**",
		"!src/**/tests",
		"!src/**/*.md",
		"!src/.vitepress/cache",
		"!src/.vitepress/dist"
	]
}