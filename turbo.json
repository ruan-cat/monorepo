{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"outputs": [
				"**/dist/**"
			],
			"dependsOn": [
				"^build"
			]
		},
		"build:docs": {
			"outputs": [
				"**/.vitepress/dist/**",
				"**/.vuepress/dist/**"
			],
			"dependsOn": [
				"^build"
			]
		},
		"vp-update": {
			"dependsOn": []
		},
		"//#changeset:publish": {
			"cache": false,
			"dependsOn": [
				"^build"
			]
		},
		"//#do-publish": {
			"dependsOn": [
				"//#changeset:publish"
			]
		}
	}
}